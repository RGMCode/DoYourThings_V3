/* InfoView */
"manual_title" = "Hướng dẫn sử dụng";
"manual_intro_title" = "Giới thiệu";
"manual_intro_text" = "Chào mừng bạn đến với DoYourThings! \nỨng dụng này được thiết kế để giúp bạn quản lý công việc hàng ngày một cách hiệu quả. Bạn có thể tạo, phân loại và ưu tiên các công việc, cũng như thiết lập thông báo để nhắc bạn về các thời hạn và sự kiện sắp tới. Tất cả dữ liệu – công việc, danh mục và cài đặt – được lưu trữ cục bộ trên thiết bị của bạn và không được truyền lên đám mây.";
"manual_categories_title" = "Danh mục";
"manual_categories_text" = "Khi ứng dụng khởi động, các danh mục mặc định như 'Riêng tư' và 'Công việc' sẽ được hiển thị. Bạn có thể sử dụng các danh mục này hoặc tạo danh mục riêng để tổ chức các công việc của mình. Mỗi danh mục có thể được gán một màu sắc duy nhất, cho phép bạn phân biệt các công việc một cách trực quan chỉ với một cái nhìn. Ngoài ra, bạn cũng có thể chỉnh sửa (thay đổi tên và màu sắc) hoặc xóa danh mục khi chúng không còn cần thiết.";
"manual_tasks_title" = "Công việc";
"manual_tasks_text" = "Với DoYourThings, bạn có thể dễ dàng tạo, chỉnh sửa và xóa công việc. Khi tạo một công việc, bạn cung cấp tiêu đề, mô tả chi tiết và mức độ ưu tiên để giúp đánh giá tầm quan trọng của công việc đó. Ngoài ra, bạn có thể đặt giờ riêng cho nhắc nhở và thời hạn. Vui lòng đảm bảo rằng các giờ được đặt vào tương lai, nếu không sẽ không có thông báo nào được kích hoạt.";
"manual_notifications_title" = "Thông báo";
"manual_notifications_text" = "Ứng dụng tự động sắp xếp các thông báo cho nhắc nhở và thời hạn dựa trên các giờ mà bạn nhập vào. Bạn cần điền đầy đủ các trường ngày và giờ tương ứng để nhận thông báo. Nếu nhập các giờ không hợp lệ hoặc đã qua, không có thông báo nào được kích hoạt. Nhấn vào thông báo sẽ mở trực tiếp phần xem chi tiết của công việc tương ứng, cho phép bạn phản hồi nhanh chóng.";
"manual_settings_title" = "Cài đặt";
"manual_settings_text" = "Trong cài đặt, bạn có thể tùy chỉnh giao diện của ứng dụng. Bạn có thể chọn giữa chế độ Sáng và Tối cũng như thiết lập màu của các biểu tượng theo sở thích cá nhân. Ngoài ra, bạn có thể quản lý danh mục của mình bằng cách tạo mới, chỉnh sửa hoặc xóa, giúp ứng dụng được điều chỉnh phù hợp với nhu cầu cá nhân của bạn.";
"manual_local_storage_title" = "Lưu trữ cục bộ";
"manual_local_storage_text" = "Tất cả các công việc, danh mục và cài đặt được lưu trữ cục bộ trên thiết bị của bạn, đảm bảo rằng dữ liệu của bạn vẫn luôn có sẵn ngay cả sau khi ứng dụng được khởi động lại. Điều này cho phép bạn kiểm soát hoàn toàn thông tin của mình.";
